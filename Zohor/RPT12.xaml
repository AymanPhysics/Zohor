            <ComboBox Height="28" HorizontalAlignment="Center" IsSynchronizedWithCurrentItem="{x:Null}" Margin="200,292,278,11" x:Name="IsStopped" TabIndex="0" VerticalAlignment="Center" Width="155" IsEditable="False" >
