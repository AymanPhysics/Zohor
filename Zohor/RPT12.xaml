                        <GradientStop Color="#37FFFF00" Offset="1" />
                    </LinearGradientBrush>
                </TextBox.Background>
            </TextBox>
            <CheckBox Content="له رصيد" Height="28" HorizontalAlignment="Center" x:Name="P" TabIndex="5" VerticalAlignment="Center" Width="120" Margin="74,274,439,29" Foreground="#FF078200" IsChecked="True" />
            <CheckBox Content="أرصدة صفرية" Height="28" HorizontalAlignment="Center" x:Name="Z" TabIndex="6" VerticalAlignment="Center" Width="120" Margin="200,274,313,29" Foreground="#FF078200" IsChecked="True" />
