                                        <CheckBox Content="Yes" Height="16" HorizontalAlignment="Center" Margin="20,25,344,0" x:Name="Y5" TabIndex="2" VerticalAlignment="Top" Width="50" IsTabStop="False" />
                                        <CheckBox Content="Yes" Height="16" HorizontalAlignment="Center" Margin="216,25,148,0" x:Name="Y8" TabIndex="2" VerticalAlignment="Top" Width="50" IsTabStop="False" />
                                        <Label BorderBrush="#FF078200" BorderThickness="1" Content="Remarks" Foreground="#FF078200" Height="28" HorizontalContentAlignment="Center" Margin="10,64,284,0" x:Name="lblHomePhone_Copy2" UseLayoutRounding="True" Width="120" HorizontalAlignment="Center" VerticalAlignment="Top" />
                                        <TextBox Height="Auto" Margin="140,64,10,10" MaxLength="1000" x:Name="Remarks1b" TabIndex="13" HorizontalAlignment="Center" VerticalAlignment="Stretch" Width="264" AutoWordSelection="True"
                                        	AcceptsReturn="True" TextWrapping="Wrap" VerticalScrollBarVisibility="Visible"  Style="{DynamicResource RemarksTextBox}" >
                                            <TextBox.Background>
                                                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                                    <GradientStop Color="White" Offset="0" />
                                                    <GradientStop Color="#37FFFF00" Offset="1" />
                                                </LinearGradientBrush>
                                            </TextBox.Background>
                                        </TextBox>

                                    </Grid>
                                </Grid>
                            </TabItem>
                        </TabControl>
                        <TabControl x:Name="TabControl2_Copy2" Height="103" HorizontalContentAlignment="Center" UseLayoutRounding="True" VerticalAlignment="Top" HorizontalAlignment="Center" Width="432" Margin="485,178,-14,0" >
                            <TabItem Header="Others" x:Name="TabItem5">
                                <Grid Background="{DynamicResource Bg}" Margin="0">
                                    <Grid Margin="5">
                                        <Grid.Background>
                                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                                <GradientStop Color="White" Offset="0" />
                                                <GradientStop Color="#FF6DD9FF" Offset="1" />
                                            </LinearGradientBrush>
                                        </Grid.Background>
                                        <WindowsFormsHost  x:Name="WFH4" Background="{x:Null}" TabIndex="333" Margin="0,0,0,0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
                                        <Button Content="Risk"  Margin="266,10,10,0" x:Name="btnRisk" TabIndex="50" VerticalAlignment="Top" HorizontalAlignment="Center" Width="138" Background="{DynamicResource BgSave}" IsTabStop="False" Height="44"/>
                                        <Button Content="Other Info"  Margin="64,10,212,0" x:Name="btnOtherInfo" TabIndex="50" VerticalAlignment="Top" HorizontalAlignment="Center" Width="138" Background="{DynamicResource BgSave}" IsTabStop="False" Height="44"/>
                                        <CheckBox Content="Yes" Height="16" HorizontalAlignment="Center" Margin="14,25,350,0" x:Name="Y9" TabIndex="2" VerticalAlignment="Top" Width="50" IsTabStop="False" />
                                        <CheckBox Content="Yes" Height="16" HorizontalAlignment="Center" Margin="208,25,156,0" x:Name="Y10" TabIndex="2" VerticalAlignment="Top" Width="50" IsTabStop="False" />

                                    </Grid>
                                </Grid>
                            </TabItem>
                        </TabControl>
                        <Label BorderBrush="#FF078200" BorderThickness="1" Content="Remarks" Foreground="#FF078200" Height="28" HorizontalContentAlignment="Center" Margin="502,360,247,0" x:Name="lblHomePhone_Copy" UseLayoutRounding="True" Width="154" HorizontalAlignment="Center" VerticalAlignment="Top" />
                        <TextBox Height="Auto" Margin="660,360,10,61" MaxLength="1000" x:Name="Remarks1a" TabIndex="13" HorizontalAlignment="Center" VerticalAlignment="Stretch" Width="233" AutoWordSelection="True"
                        	AcceptsReturn="True" TextWrapping="Wrap" VerticalScrollBarVisibility="Visible"  Style="{DynamicResource RemarksTextBox}" >
                            <TextBox.Background>
                                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                    <GradientStop Color="White" Offset="0" />
                                    <GradientStop Color="#37FFFF00" Offset="1" />
                                </LinearGradientBrush>
                            </TextBox.Background>
                        </TextBox>
                        <CheckBox Content="Yes" Height="16" HorizontalAlignment="Center" Margin="127,25,726,0" x:Name="Y1" TabIndex="2" VerticalAlignment="Top" Width="50" IsTabStop="False" />
                        <CheckBox Content="Yes" Height="16" HorizontalAlignment="Center" Margin="451,25,402,0" x:Name="Y2" TabIndex="2" VerticalAlignment="Top" Width="50" IsTabStop="False" />
                        <CheckBox Content="Yes" Height="16" HorizontalAlignment="Center" Margin="26,0,827,23" x:Name="Y11" TabIndex="2" VerticalAlignment="Bottom" Width="50" IsTabStop="False" />
                        <CheckBox Content="Yes" Height="16" HorizontalAlignment="Center" Margin="303,0,550,23" x:Name="Y12" TabIndex="2" VerticalAlignment="Bottom" Width="50" IsTabStop="False" />
                        <CheckBox Content="Yes" Height="16" HorizontalAlignment="Center" Margin="581,0,272,23" x:Name="Y13" TabIndex="2" VerticalAlignment="Bottom" Width="50" IsTabStop="False" />


                    </Grid>
                </Grid>

            </TabItem>

        </TabControl>
        <Button Content="Investigations"  Margin="153,0,408,58" x:Name="btnInvestigations" TabIndex="50" VerticalAlignment="Bottom" HorizontalAlignment="Center" Width="124" Background="{DynamicResource BgSave}" IsTabStop="False" Height="40"/>
        <Button Content="Complaint"  Margin="24,0,537,58" x:Name="btnComplaint" TabIndex="50" VerticalAlignment="Bottom" HorizontalAlignment="Center" Width="124" Background="{DynamicResource BgSave}" IsTabStop="False" Height="40"/>
        <Button Content="Ultra Sound"  Margin="271,0,526,10" x:Name="btnUltraSound" TabIndex="50" VerticalAlignment="Bottom" HorizontalAlignment="Center" Width="124" Background="{DynamicResource BgSave}" IsTabStop="False" Height="40"/>
        <Button Content="Drugs &amp; Doses"  Margin="282,0,279,58" x:Name="btnDrugsAndDoses" TabIndex="50" VerticalAlignment="Bottom" HorizontalAlignment="Center" Width="124" Background="{DynamicResource BgSave}" IsTabStop="False" Height="40"/>
        <Button Content="Investigations Request"  Margin="400,0,359,10" x:Name="btnNeededLabTests" TabIndex="50" VerticalAlignment="Bottom" HorizontalAlignment="Center" Width="162" Background="{DynamicResource BgSave}" IsTabStop="False" Height="40"/>
        <Button Content="Investigations Entry"  Margin="116,0,655,10" x:Name="btnInvestigationsEntry" TabIndex="50" VerticalAlignment="Bottom" HorizontalAlignment="Center" Width="150" Background="{DynamicResource BgSave}" IsTabStop="False" Height="40"/>
    </Grid>
</UserControl>
