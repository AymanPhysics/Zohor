<UserControl x:Class="InFertility"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="625.997" d:DesignWidth="920.796">
    <Grid Background="{DynamicResource Bg}">
        <TabControl x:Name="TabControl4" HorizontalContentAlignment="Center" Margin="0,0,0,102">
            <TabItem Header="InFertility" x:Name="TabItem4"  >
                <Grid Background="{DynamicResource Bg}">
                    <Grid Margin="5">
                        <Grid.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="White" Offset="0" />
                                <GradientStop Color="#FF6DD9FF" Offset="1" />
                            </LinearGradientBrush>
                        </Grid.Background>
                        <Button Content="IVF Lab"  Margin="281,0,297,10" x:Name="btnIVFLab" TabIndex="50" VerticalAlignment="Bottom" HorizontalAlignment="Center" Width="171" Background="{DynamicResource BgSave}" IsTabStop="False" Height="44"/>
                        <Button Content="ICSI"  Margin="81,0,651,10" x:Name="btnICSI" TabIndex="50" VerticalAlignment="Bottom" HorizontalAlignment="Center" Width="171" Background="{DynamicResource BgSave}" IsTabStop="False" Height="44"/>
                        <Button Content="Follicular Scaning"  Margin="636,0,96,10" x:Name="btnFollicularScaning" TabIndex="50" VerticalAlignment="Bottom" HorizontalAlignment="Center" Width="171" Background="{DynamicResource BgSave}" IsTabStop="False" Height="44"/>
