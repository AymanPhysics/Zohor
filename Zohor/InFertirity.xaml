                     AcceptsReturn="True" TextWrapping="Wrap" VerticalScrollBarVisibility="Visible"  Style="{DynamicResource RemarksTextBox}" >
                            <TextBox.Background>
                                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                    <GradientStop Color="White" Offset="0" />
                                    <GradientStop Color="#37FFFF00" Offset="1" />
                                </LinearGradientBrush>
                            </TextBox.Background>
                        </TextBox>
                        <TabControl x:Name="TabControl2_Copy1" Height="177" HorizontalContentAlignment="Center" UseLayoutRounding="True" VerticalAlignment="Top" HorizontalAlignment="Center" Width="432" Margin="26,178,445,0" >
                            <TabItem Header="Previous ART" x:Name="TabItem3">
                                <Grid Background="{DynamicResource Bg}" Margin="0">
                                    <Grid Margin="5">
                                        <Grid.Background>
                                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                                <GradientStop Color="White" Offset="0" />
                                                <GradientStop Color="#FF6DD9FF" Offset="1" />
                                            </LinearGradientBrush>
                                        </Grid.Background>
                                        <WindowsFormsHost  x:Name="WFH3" Background="{x:Null}" TabIndex="333" Margin="0,0,0,0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
