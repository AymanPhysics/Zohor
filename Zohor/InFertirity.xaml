                                        <CheckBox Content="Yes" Height="16" HorizontalAlignment="Center" Margin="12,25,352,23" x:Name="Y4" TabIndex="2" VerticalAlignment="Center" Width="50" IsTabStop="False" />
                                        <CheckBox Content="Yes" Height="16" HorizontalAlignment="Center" Margin="214,25,150,23" x:Name="Y7" TabIndex="2" VerticalAlignment="Center" Width="50" IsTabStop="False" />

                                    </Grid>
                                </Grid>
                            </TabItem>
                        </TabControl>
