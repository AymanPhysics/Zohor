                                        <CheckBox Content="Yes" Height="16" HorizontalAlignment="Center" Margin="12,25,352,23" x:Name="Y3" TabIndex="2" VerticalAlignment="Center" Width="50" IsTabStop="False" />
                                        <CheckBox Content="Yes" Height="16" HorizontalAlignment="Center" Margin="214,25,150,23" x:Name="Y6" TabIndex="2" VerticalAlignment="Center" Width="50" IsTabStop="False" />

                                    </Grid>
                                </Grid>
                            </TabItem>
                        </TabControl>
                        <TabControl x:Name="TabControl2_Copy" Height="103" HorizontalContentAlignment="Center" UseLayoutRounding="True" VerticalAlignment="Top" HorizontalAlignment="Center" Width="432" Margin="485,69,-14,0" >
                            <TabItem Header="Tubal &amp; Peritioneal Factor" x:Name="TabItem1">
                                <Grid Background="{DynamicResource Bg}" Margin="0">
                                    <Grid Margin="5">
                                        <Grid.Background>
                                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                                <GradientStop Color="White" Offset="0" />
                                                <GradientStop Color="#FF6DD9FF" Offset="1" />
                                            </LinearGradientBrush>
                                        </Grid.Background>
                                        <WindowsFormsHost  x:Name="WFH1" Background="{x:Null}" TabIndex="333" Margin="0,0,0,0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
