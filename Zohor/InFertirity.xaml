                            <TabItem Header="Utrine Factor" x:Name="TabItem2">
                                <Grid Background="{DynamicResource Bg}" Margin="0">
                                    <Grid Margin="5">
                                        <Grid.Background>
                                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                                <GradientStop Color="White" Offset="0" />
                                                <GradientStop Color="#FF6DD9FF" Offset="1" />
                                            </LinearGradientBrush>
                                        </Grid.Background>
                                        <WindowsFormsHost  x:Name="WFH2" Background="{x:Null}" TabIndex="333" Margin="0,0,0,0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
                                        <Button Content="Ultra Sound"  Margin="68,10,208,0" x:Name="btnUtrineFactorUltraSound" TabIndex="50" VerticalAlignment="Top" HorizontalAlignment="Center" Width="138" Background="{DynamicResource BgSave}" IsTabStop="False" Height="44"/>
