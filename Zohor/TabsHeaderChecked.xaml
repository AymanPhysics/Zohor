<UserControl x:Class="TabsHeaderChecked"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="23" d:DesignWidth="149">
    <Grid Name="Grid1">
        <TextBlock Height="21" Name="TextBlock1" Text="TextBlock" VerticalAlignment="Top" TextAlignment="Center" Margin="43,2,0,0" Width="Auto" HorizontalAlignment="Stretch"/>
        <CheckBox Name="CheckBox1" IsChecked="True" HorizontalAlignment="Left" Width="37" HorizontalContentAlignment="Center" VerticalContentAlignment="Center">
            <CheckBox.Foreground>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Black" Offset="0" />
                    <GradientStop Color="White" Offset="1" />
                    <GradientStop Color="White" Offset="0.027" />
                </LinearGradientBrush>
            </CheckBox.Foreground>
        </CheckBox>
    </Grid>
</UserControl>
